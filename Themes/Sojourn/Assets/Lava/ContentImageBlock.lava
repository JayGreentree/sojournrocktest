{% for item in Items %}
<div class="section">
    <div class="content-block image-right">
        <div class="image-block wow slideInRight">{{ item | Attribute:'Image' }}</div>
        <div class="container">
            <div class="row">
                <div class="col-md-6">
                    <h2>{{ item.Title }}</h2>
                    {{ item.Content }}
                </div>
            </div>
        </div>
    </div>
</div>
{% endfor %}